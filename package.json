{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "pnpm lint && pnpm type-check && pnpm test",
    "prepare": "husky install",
    "analyze": "ANALYZE=true pnpm build",
