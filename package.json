{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn generate:rss && yarn generate:sitemap",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "analyze": "ANALYZE=true pnpm build",
    "generate:rss": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "generate:sitemap": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.2",
    "@giphy/react-components": "^5.4.0",
    "@headlessui/react": "^1.4.2",
    "@prisma/client": "^3.5.0",
    "@radix-ui/react-icons": "^1.0.3",
    "@react-hookz/web": "^12.0.0",
    "@stitches/react": "^1.2.5",
    "algoliasearch": "^4.11.0",
    "axios": "^0.24.0",
    "browser-monads-ts": "^1.2.5",
    "contentlayer": "^0.0.32",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.26.0",
    "mdx-embed": "^0.0.22",
    "microtip": "^0.2.2",
    "nanoid": "^3.1.30",
    "next": "^12.0.5-canary.9",
    "next-compose-plugins": "^2.2.1",
    "next-connect": "^0.11.0",
    "next-contentlayer": "^0.0.32",
    "next-plausible": "^3.1.4",
    "next-rosetta": "^1.3.1",
    "next-seo": "^4.28.1",
    "next-themes": "^0.0.15",
    "preact": "^10.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.4",
    "react-instantsearch-dom": "^6.16.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@giphy/js-types": "^4.1.0",
    "@next/bundle-analyzer": "^12.0.4",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/node": "^16.11.10",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.36",
    "@types/react-instantsearch-dom": "^6.12.0",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "8.3.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "feed": "^4.2.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-code-titles": "^1.0.3",
